{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/**/*.html",
          "/**/*.scss",
          "/**/*.ts"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups": [{
    "name": "myLingual",
    "urls": [
      "https://mylingual.me/**"
    ],
    "cacheConfig": {
      "maxAge": "6h",
      "strategy": "freshness"
    }
  }]
}
